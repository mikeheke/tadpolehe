<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!--********************************* Dao *********************************-->
	<bean id="newFileUploadTagDao" class="com.amway.frm.tag.file.dao.impl.NewFileUploadTagDao"></bean>


	<!--********************************* Service *********************************-->
	<!-- 文件上传下载Service -->
	<bean id="newFileUploadTagService" class="com.amway.frm.tag.file.service.impl.NewFileUploadTagServiceImpl"
		parent="ServiceTemplate" >
		<property name="baseDataSourceService" ref="BaseDataSourceService" />
		<property name="newFileUploadTagDao" ref="newFileUploadTagDao" />
	</bean>
	
	
	<!--********************************* Action *********************************-->
	<!-- 文件上传下载Action -->
	<bean id="newFileUploadTagAction" class="com.amway.frm.tag.file.web.action.NewFileUploadTagAction" scope="prototype">
		<property name="newFileUploadTagService" ref="newFileUploadTagService" />
	</bean>
	<!-- 文件上传下载Test Action -->
	<bean id="newFileUploadTagTestAction" class="com.amway.frm.tag.file.web.action.NewFileUploadTagTestAction" scope="prototype">
		<property name="newFileUploadTagService" ref="newFileUploadTagService" />
	</bean>
	
</beans>