<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
 
   <bean id="BdsSchemaInforAction" class="com.amway.frm.bds.web.action.BdsSchemaInforAction">
		<property name="bdsSchemaInforService" ref="BdsSchemaInforService" />
   </bean>
   <bean id="BdsSchemaInforService" class="com.amway.frm.bds.service.impl.BdsSchemaInforImpl" parent="ServiceTemplate" >
   		<property name="bdsSchemaInforDao" ref="BdsSchemaInforDao" />
   </bean>
   <bean id="BdsSchemaInforDao" class="com.amway.frm.bds.dao.impl.BdsSchemaInforDao"/>
   
   <bean id="BdsXmlStructureAction" class="com.amway.frm.bds.web.action.BdsXmlStructureAction" parent="BdsSchemaInforAction">
		<property name="bdsXmlStructureService" ref="BdsXmlStructureService" />
   </bean>
   <bean id="BdsXmlStructureService" class="com.amway.frm.bds.service.impl.BdsXmlStructureImpl" parent="ServiceTemplate" />
   
   <bean id="BdsXMLDataAction" class="com.amway.frm.bds.web.action.BdsXMLDataAction" parent="BdsXmlStructureAction">
		<property name="bdsXmlDataService" ref="BdsXmlDataService" />
   </bean>
   <bean id="BdsXmlDataService" class="com.amway.frm.bds.service.impl.BdsXmlDataImpl" parent="ServiceTemplate" >
   		<property name="bdsXmlDataDao" ref="BdsXmlDataDao" />
   </bean>
   <bean id="BdsXmlDataDao" class="com.amway.frm.bds.dao.impl.BdsXmlDataDao"/>

   <bean id="BaseDataSourceService" class="com.amway.frm.bds.service.impl.BaseDataSourceServiceImpl" parent="ServiceTemplate" >
   		<property name="bdsXmlDataDao" ref="BdsXmlDataDao"></property>
   		<property name="bdsSchemaInforService" ref="BdsSchemaInforService" />
   		<property name="bdsXmlDataService" ref="BdsXmlDataService" />
   </bean>

</beans>