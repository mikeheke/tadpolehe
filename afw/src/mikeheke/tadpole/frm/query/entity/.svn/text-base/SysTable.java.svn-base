package mikeheke.tadpole.frm.query.entity;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;

/**
 * @author huangweijin
 *
 * 2011-5-18 下午02:01:52
 */
@Entity
@javax.persistence.Table(name="USER_TABLES")
public class SysTable implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 5683585987142948818L;
	
	@Id
	@Column(name="TABLE_NAME")
	private String tableName;

	public String getTableName() {
		return tableName;
	}

	public void setTableName(String tableName) {
		this.tableName = tableName;
	}
	
}
