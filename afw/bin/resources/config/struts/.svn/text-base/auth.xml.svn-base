<struts>
  
  <package name="auth" namespace="/" extends="default">
  
      <action name="loginAction" class="LoginAction">
		  <result name="input" type="dispatcher">/common/jsp/login.jsp</result>
	      <result name="success" type="redirect">${goToSysUrl }</result>
	  </action>
  </package>
</struts>    
