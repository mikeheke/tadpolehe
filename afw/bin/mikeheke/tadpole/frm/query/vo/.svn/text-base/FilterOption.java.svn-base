package mikeheke.tadpole.frm.query.vo;

import mikeheke.tadpole.frm.query.util.QueryConstant;

/**
 * 
 * @author lenovo
 *
 */
public class FilterOption implements
		org.extremecomponents.table.cell.FilterOption {

	private Object label=QueryConstant.EMPTY_STR;
	
	private Object value=QueryConstant.EMPTY_STR;
	
	@Override
	public Object getLabel() {
		return label;
	}

	@Override
	public Object getValue() {
		return value;
	}

	public void setLabel(Object label) {
		this.label = label;
	}

	public void setValue(Object value) {
		this.value = value;
	}

	
}
